{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "40e0ae2b-646c-405c-84ce-57d392ea19b3"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "demo": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/demo/",
      "sourceRoot": "projects/demo/src",
      "prefix": "app",
      "targets": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo",
            "index": "projects/demo/src/index.html",
            "main": "projects/demo/src/main.ts",
            "polyfills": "projects/demo/src/polyfills.ts",
            "tsConfig": "projects/demo/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/demo/src/favicon.ico",
              "projects/demo/src/assets"
            ],
            "styles": [
              "projects/demo/src/styles.css",
              "node_modules/bootstrap-material-design/dist/css/bootstrap-material-design.min.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo/src/environments/environment.ts",
                  "with": "projects/demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            }
          }
        }
      }
    },
    "ngforage": {
      "projectType": "library",
      "root": "projects/ngforage",
      "sourceRoot": "projects/ngforage/src",
      "prefix": "ngf",
      "targets": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngforage/tsconfig.lib.json",
            "project": "projects/ngforage/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngforage/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverage": true,
            "main": "projects/ngforage/src/test.ts",
            "tsConfig": "projects/ngforage/tsconfig.spec.json",
            "karmaConfig": "projects/ngforage/karma.conf.js",
            "codeCoverageExclude": [
              "**/*.spec.{ts,js}",
              "**/session-storage/**/*.{ts,js}"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ngforage"
}
